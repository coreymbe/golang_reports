# Managed by Puppet
# /etc/systemd/system/golang-reports.service.d/env.conf
[Service]
Environment=dbName=<%= $golang_reports::server::db_name %>
Environment=dbUser=<%= $golang_reports::server::db_user %>
Environment=dbPass=<%= $golang_reports::server::db_pass %>
Environment=JWTSecretKey=<%= $golang_reports::server::jwt_secret %>
